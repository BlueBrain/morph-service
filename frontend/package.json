{
  "name": "morpho_tools_frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "VUE_APP_ANNOTATION_VERSION=$(npm run --silent getAnnotationVersion) vue-cli-service serve",
    "build": "VUE_APP_ANNOTATION_VERSION=$(npm run --silent getAnnotationVersion) vue-cli-service build",
    "lint": "vue-cli-service lint",
    "generateFilters": "node -e 'require(\"./generate-filter-config\").createFiltersFn()'",
    "getAnnotationVersion": "node -e 'require(\"./generate-filter-config\").getAnnotationVersion()'"
  },
  "dependencies": {
    "bulma": "^0.7.1",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "vue": "^2.5.16",
    "vue-search-select": "^2.6.1"
  },
  "devDependencies": {
    "@voerro/vue-tagsinput": "^1.5.2",
    "@vue/cli-plugin-babel": "^3.0.0-beta.10",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.10",
    "@vue/cli-service": "^3.0.0-beta.10",
    "bulma-extensions": "^1.0.31",
    "chart.js": "^2.7.2",
    "distinct-colors": "^1.0.4",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-vue": "^4.5.0",
    "file-saver": "^1.3.8",
    "jszip": "^3.1.5",
    "lodash": "^4.17.10",
    "vue-router": "^3.0.1",
    "vue-select": "^2.4.0",
    "vue-template-compiler": "^2.5.13",
    "vue2-dropzone": "^3.2.2"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "plugins": [
      "vue"
    ],
    "extends": [
      "google",
      "plugin:vue/recommended"
    ],
    "rules": {
      "indent": [
        "error",
        2,
        {
          "MemberExpression": 0
        }
      ],
      "no-invalid-this": 0,
      "require-jsdoc": 0,
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "quote-props": [
        "error",
        "as-needed"
      ],
      "semi": [
        "error",
        "always"
      ],
      "max-len": 0,
      "brace-style": [
        "error",
        "1tbs",
        {
          "allowSingleLine": true
        }
      ],
      "vue/require-prop-types": 0,
      "vue/require-default-prop": 0
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
